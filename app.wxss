/* 导入 iconfont样式 */
@import "./style/iconfont.wxss";

/* 小程序不支持 通配符'*' 所以手写 */
page,
view,
text,
swiper,
swiper-item,
image,
navigator,
input {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 主题颜色 可以使用 原生css 和 wxss 都支持变量*/
page {
  /* 统一主题颜色 */
  --themeColor: #E43130;
  /* 统一字体 大小 这里的设计稿 750px */
  font-size: 30rpx;
}