.container_address .address_btn {
  padding: 20rpx;
  display: flex;
  justify-content: center;
}
.container_address .address_btn navigator {
  border: 1rpx solid var(--themeColor);
  height: 120rpx;
  width: 60%;
  font-size: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: var(--themeColor);
}
.container_address .user_info_row {
  display: flex;
  padding: 20rpx;
}
.container_address .user_info_row .user_info {
  flex: 5;
}
.container_address .user_info_row .user_info .user_detailInfo {
  display: flex;
  justify-content: left;
}
.container_address .user_info_row .user_info .user_detailInfo text {
  padding-right: 35rpx;
  font-size: 32rpx;
  font-weight: bold;
}
.container_address .user_info_row .user_info .text {
  margin: 7rpx 0;
  font-size: 26rpx;
  color: #666;
  font-weight: bold;
}
.container_address .user_info_row .right {
  width: 60rpx;
  height: 60rpx;
}
.container_address .user_info_row .right .tools {
  margin: 75rpx 10rpx 0 10rpx;
  width: 100%;
  height: 100%;
}
.container_address .user_info_row .right .tools image {
  width: 100%;
  height: 100%;
}
.cart_container .cart_title {
  padding: 20rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: var(--themeColor);
  border-top: 1px solid currentColor;
  border-bottom: 1px solid currentColor;
}
.cart_container .cart_main .cart_item {
  display: flex;
  padding-right: 10rpx;
  border-bottom: 1rpx solid #ccc;
}
.cart_container .cart_main .cart_item .cart_img_wrap {
  flex: 2;
  margin: 15rpx -20rpx 15rpx 15rpx;
}
.cart_container .cart_main .cart_item .cart_img_wrap view image {
  width: 85%;
}
.cart_container .cart_main .cart_item .cart_info_wrap {
  flex: 4;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.cart_container .cart_main .cart_item .cart_info_wrap .goods_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #666;
}
.cart_container .cart_main .cart_item .cart_info_wrap .goods_price_wrap {
  display: flex;
  justify-content: space-between;
}
.cart_container .cart_main .cart_item .cart_info_wrap .goods_price_wrap .goods_price {
  color: var(--themeColor);
  font-size: 34rpx;
  font-weight: 600;
}
.cart_container .cart_main .cart_item .cart_info_wrap .goods_price_wrap .cart_num_tool {
  display: flex;
}
.cart_container .cart_main .cart_item .cart_info_wrap .goods_price_wrap .cart_num_tool .num_edit {
  border-radius: 10rpx;
  width: 55rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #ccc;
}
.cart_container .cart_main .cart_item .cart_info_wrap .goods_price_wrap .cart_num_tool .goods_num {
  width: 55rpx;
  height: 55rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_container .cart_main .kong_main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.cart_container .cart_main .kong_main image {
  width: 90%;
  margin: 40rpx;
}
.cart_container .cart_main .kong_main navigator {
  font-size: 40rpx;
  color: var(--themeColor);
  font-weight: 500;
  margin: 20rpx 0;
}
.footer_tool {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  background-color: #fff;
  display: flex;
  border-top: 1px solid #ccc;
}
.footer_tool .all_chk_wrap {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.footer_tool .total_price_wrap {
  flex: 5;
  text-align: right;
  padding-right: 15rpx;
}
.footer_tool .total_price_wrap .total_price {
  font-size: 27rpx;
}
.footer_tool .total_price_wrap .total_price .total_price_text {
  color: var(--themeColor);
  font-size: 34rpx;
  font-weight: 600;
  padding-left: 10rpx;
}
.footer_tool .total_price_wrap .yunfei {
  color: #666;
  padding: 5rpx 0 0 0;
  font-size: 25rpx;
}
.footer_tool .order_pay_wrap {
  flex: 3;
  background-color: var(--themeColor);
  color: #fff;
  font-size: 32rpx;
  font-weight: 600;
  display: flex;
  justify-content: center;
  align-items: center;
}
page {
  padding-bottom: 90rpx;
}
