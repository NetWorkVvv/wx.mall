.tabs_s .tabs_title {
  display: flex;
  align-items: center;
  background-color: #fff;
}
.tabs_s .tabs_title .tabs_title_item {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  padding: 10rpx 0;
  font-weight: bold;
  flex: 1;
}
.tabs_s .tabs_title .active {
  font-weight: bold;
  color: var(--themeColor);
  border-bottom: 5rpx solid currentColor;
}
.tabs_s .tabs_title .choose {
  color: var(--themeColor);
  display: flex;
}
